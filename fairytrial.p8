--game loop
function _init()
   init_menu()
   make_player()
   music(0)
end


function init_menu()
 _update = update_menu
 _draw = draw_menu
end

function init_game()
  -- set the fairy's starting position
  map_setup()
  music(7)
  text_setup()
  chat_setup()
  
    
  game_win=false
  game_over=false

   -- set state
   fog=blankmap(0,31,1,15)
   fogcolor=blankmap(0,255,0,255)
   _update = update_game
   _draw = draw_game
   
end

function update_menu()
  
 
 if btnp(‚ùé) then
   init_game()  
 end
end

function update_game()
 if (not game_over) then
  if (not active_text) then
   if (not active_chat) then
    update_map()
    fly()	
    move_player()
    check_win_lose()
   end
  end 
 else
  if (btnp(‚ùé)) then 
    extcmd("reset")
  end
 end  
end

function draw_menu()
 cls()
 
for n=1,3 do
  circ(rnd(128), rnd(64), rnd(2), rnd{6,7,12})
end
 fly()
 spr(p.sprite, 7*8, 3*8)
for n=1,3 do
 print("‚úΩfairy trial‚úΩ",34,40,rnd{2,6,7,12,13,14})
end 
 print("press ‚ùé to start",30,64,7)
end

function draw_game()
 cls()
 
 if (not game_over) then
  draw_map()
  draw_player()
  draw_text()
  draw_chat()
--  if (btn(‚ùé)) show_inventory()
 else
  draw_win_lose()
 end
end



